{
	"name": "datass",
	"version": "0.0.1",
	"author": {
		"name": "rokki",
		"email": "tasteink@proton.me",
		"url": "https://github.com/tasteee"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tasteee/datass.git"
	},
	"main": "./dist/datass.cjs",
	"module": "./dist/datass.esm.js",
	"jsdelivr": "./dist/datass.iife.js",
	"exports": {
		".": {
			"require": "./dist/datass.cjs",
			"import": "./dist/datass.esm.js",
			"types": "./dist/datass.d.ts"
		},
		"./dist/": {
			"import": "./dist/",
			"require": "./dist/",
			"types": "./dist/"
		}
	},
	"scripts": {
		"dev": "vite --host",
		"prebuild": "rm -rf dist/*",
		"build": "tsc && vite build",
		"postbuild": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"format": "prettier . --write",
		"upgrade": "upgradeps",
		"release": "npm run build && np"
	},
	"bugs": {
		"url": "https://github.com/tasteee/datass/issues"
	},
	"description": "datass helps you finish quicker. its lightweight, flexible, and unopinionated.",
	"files": [
		"dist",
		"README.md",
		"LICENSE.md",
		"CHANGELOG.md",
		"src",
		"package.json"
	],
	"funding": [
		{
			"type": "individual",
			"url": "https://support.example.com"
		}
	],
	"homepage": "https://github.com/tasteee/datass#readme",
	"keywords": [
		"some",
		"keywords"
	],
	"license": "MIT",
	"private": false,
	"sponsor": {
		"url": "https://x.com/ken_wheeler"
	},
	"type": "module",
	"types": "./dist/datass.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/datass.d.ts"
			]
		}
	},
	"typings": "./dist/datass.d.ts",
	"devDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.5.0",
		"@vitest/coverage-v8": "^2.0.5",
		"dts-bundle-generator": "^9.5.1",
		"jsdom": "^24.1.1",
		"lint-staged": "^15.2.9",
		"np": "^10.0.6",
		"prettier": "^3.3.3",
		"terser": "^5.31.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"upgradeps": "^2.0.6",
		"vite": "^5.0.12",
		"vitest": "^2.0.0"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	}
}
